{
  "name": "esbox",
  "description": "ES2016 in a box",
  "version": "0.4.0",
  "ava": {
    "babel": false,
    "files": "dist/test/**/*.test.js",
    "source": [
      "dist/lib/**/*.js",
      "fixture/**/*.js"
    ]
  },
  "bin": {
    "esbox": "dist/lib/cli.js"
  },
  "dependencies": {
    "ansi-escapes": "^1.3.0",
    "babel-polyfill": "^6.7.2",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-es2015-node4": "^2.0.3",
    "babel-preset-es2015-node5": "^1.1.2",
    "babel-preset-stage-0": "^6.5.0",
    "babel-register": "^6.7.2",
    "builtin-modules": "^1.1.1",
    "chalk": "^1.1.1",
    "cli-color": "^1.1.0",
    "execa": "^0.2.2",
    "figures": "^1.4.0",
    "is-absolute": "^0.2.3",
    "lodash": "^4.6.1",
    "loud-rejection": "^1.3.0",
    "minimist": "^1.2.0",
    "path-exists": "^2.1.0",
    "pretty-hrtime": "^1.0.2",
    "sane": "^1.3.3",
    "stack-trace": "0.0.9",
    "subdir": "0.0.3",
    "update-notifier": "^0.6.1"
  },
  "devDependencies": {
    "ava": "^0.13.0",
    "babel-cli": "^6.6.5",
    "babel-eslint": "^6.0.0-beta.5",
    "eslint": "~2.2.0",
    "eslint-config-airbnb": "^6.1.0",
    "eslint-plugin-babel": "^3.1.0",
    "rimraf": "^2.5.2"
  },
  "engines": {
    "node": ">=0.12"
  },
  "files": [
    "dist/lib"
  ],
  "license": "MIT",
  "repository": "callumlocke/esbox",
  "scripts": {
    "build": "rimraf dist && babel src --out-dir dist --source-maps",
    "build:watch": "rimraf dist && babel src --out-dir dist --watch --source-maps",
    "lint": "eslint .",
    "prepublish": "npm run build",
    "test": "ava",
    "test:watch": "ava --watch"
  }
}
